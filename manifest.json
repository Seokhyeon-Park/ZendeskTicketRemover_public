{
    "author": "Seokhyeon Park",
    "name": "Zendesk ticket remover",
    "description": "Close all tickets on topbar.",
    "version": "1.01",
    "manifest_version": 3,
    "icons": {
        "16": "zendesk-16.png",
        "32": "zendesk-48.png",
        "48": "zendesk-48.png",
        "128": "zendesk-128.png"
    },
    "action": {
        "default_title": "Close all tickets..."
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    }
}